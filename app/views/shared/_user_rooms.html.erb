<section class="flex flex-col w-full items-center">
    <h3 class="text-xs font-semibold mt-3 text-center mb-2">My Rooms</h3>

    <div class="flex flex-col px-5 gap-2 w-full">
        <% @rooms.each do |room| %>
            <%= render 'shared/room', room: room %>
        <% end %>
    </div>
</section>
